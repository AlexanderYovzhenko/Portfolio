{
  "name": "my-portfolio",
  "homepage": "https://username.github.io/MyPortfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{tsx,ts}\" \"test/**/*.{tsx,ts}\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx"
    ]
  },
  "semi": false,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "i18next": "^21.8.11",
    "i18next-browser-languagedetector": "^6.1.4",
    "moment": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.3",
    "react-intersection-observer": "^9.3.3",
    "react-live-clock": "^6.0.1",
    "react-moment": "^1.1.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.8.7",
    "react-select": "^5.3.2",
    "swiper": "^8.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.39",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^18.0.12",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-clock": "^3.0.1",
    "@types/react-dom": "^18.0.5",
    "@types/react-scroll": "^1.8.3",
    "@types/react-select": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "classnames": "^2.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^4.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.3",
    "typescript": "^4.7.3"
  }
}
